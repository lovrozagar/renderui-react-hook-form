{
  "env": {
    "browser": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["*.tsx"],
      "rules": {
        "max-lines": ["error", { "max": 300, "skipBlankLines": false, "skipComments": false }]
      }
    }
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["tsconfig.json"]
  },
  "plugins": ["no-relative-import-paths", "@typescript-eslint"],
  "rules": {}
}
